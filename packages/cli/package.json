{
  "name": "@usk/cli",
  "version": "0.1.0",
  "description": "Command-line interface for Universal Skill Kit",
  "type": "module",
  "main": "./dist/index.js",
  "module": "./dist/index.mjs",
  "types": "./dist/index.d.ts",
  "bin": {
    "usk": "./dist/cli.js"
  },
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.mjs",
      "require": "./dist/index.js"
    }
  },
  "files": [
    "dist",
    "README.md"
  ],
  "repository": {
    "type": "git",
    "url": "git+https://github.com/JiangDing1990/universal-skill-kit.git",
    "directory": "packages/cli"
  },
  "homepage": "https://github.com/JiangDing1990/universal-skill-kit#readme",
  "bugs": {
    "url": "https://github.com/JiangDing1990/universal-skill-kit/issues"
  },
  "scripts": {
    "build": "tsup",
    "dev": "tsup --watch",
    "test": "vitest run",
    "test:watch": "vitest",
    "type-check": "tsc --noEmit"
  },
  "dependencies": {
    "@usk/core": "workspace:*",
    "@usk/utils": "workspace:*",
    "chalk": "^5.3.0",
    "commander": "^11.1.0",
    "diff": "^5.1.0",
    "inquirer": "^9.2.12",
    "ora": "^7.0.1"
  },
  "devDependencies": {
    "@types/diff": "^5.0.9",
    "@types/inquirer": "^9.0.7",
    "@types/node": "^20.10.5",
    "tsup": "^8.0.1",
    "typescript": "^5.3.3",
    "vitest": "^1.0.4"
  },
  "keywords": [
    "cli",
    "command",
    "skill",
    "usk",
    "claude-code",
    "codex",
    "converter",
    "developer-tools",
    "terminal"
  ],
  "author": "USK Team",
  "license": "MIT"
}
